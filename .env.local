# Frontend Environment Variables
# Copy this file to .env.local and update values as needed

# Authentication Service (ms_autentificacion)
REACT_APP_AUTH_GRAPHQL_URL=http://localhost:8000/graphql

# Main GraphQL API Endpoint (ms_websocket)
REACT_APP_GRAPHQL_URL=http://localhost:4000/graphql

# REST API Endpoint
REACT_APP_API_URL=http://localhost:5000/api/v1

# Environment
REACT_APP_ENV=development

# Log Level
REACT_APP_LOG_LEVEL=debug

# WebSocket URL (for real-time features)
REACT_APP_WS_URL=http://localhost:4000

# Authentication
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_AUTH_TOKEN_TYPE_KEY=auth_token_type
