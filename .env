# Frontend Environment Variables

# Apollo Federation Gateway - Unified GraphQL API
# This is the single entry point for all GraphQL queries
REACT_APP_GRAPHQL_URL=http://localhost:4000/graphql

# Authentication Service (ms_autentificacion) - Direct endpoint for auth
# (Can be used as fallback if not going through federation gateway)
REACT_APP_AUTH_GRAPHQL_URL=http://localhost:8000/graphql

# REST API Endpoint
REACT_APP_API_URL=http://localhost:5000/api/v1

# WebSocket URL (ms_websocket)
REACT_APP_WS_URL=http://localhost:4004

# Environment
REACT_APP_ENV=development

# Log Level
REACT_APP_LOG_LEVEL=debug

# Authentication
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_AUTH_TOKEN_TYPE_KEY=auth_token_type

# Dev Server Configuration
DANGEROUSLY_DISABLE_HOST_CHECK=true
HOST=localhost
PORT=3000

